{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
      "no-underscore-dangle": [2, {"allow": ["_content"]}],
      "no-console": [1, {"allow": ["error"]}]
    }
}
